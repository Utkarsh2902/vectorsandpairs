ðŸ˜ŽWAP to sort on the behalf of left and right pair element using vector and pairs
#include <iostream>
#include <vector>
using namespace std;
int main()
{
 vector<pair<int,int>> v={{1,2},{5,4},{7,9}};
 vector<int> v1;
  for(int i=0;i<3;i++)
  {
      if(v[i].first<v[i].second)
      {
      v1.push_back(v[i].first);
      v1.push_back(v[i].second);
      }
      else if(v[i].first>v[i].second)
      {
      v1.push_back(v[i].second);
      v1.push_back(v[i].first);
      }
  }
  for(int i=0;i<v1.size();i++)
  {
      cout<<v1[i];
  }

    return 0;
}






ðŸ˜ŽWAP to sort on the behalf of sum using vector and pairs
#include <iostream>
#include <vector>
using namespace std;
int main()
{
  vector<pair<int,int>> v={{1,2},{5,4},{7,9}};
  vector<int> v1;
  for(int i=0;i<=2;i++)
  {
      int c,d;
      c=v[i].first+v[i].second;
      d=v[i+1].first+v[i+1].second;
      if(c<d)
      {
      v1.push_back(c);
      }
      else
      {
      v1.push_back(d);
      }
      c=0;
      d=0;
  }
  for(int i=0;i<v1.size();i++)
  {
      cout<<v1[i];
      cout<<"\n";
  }
    return 0;
}






ðŸ˜ŽWAP nested vector and pair dynamically
#include<bits/stdc++.h>
using namespace std;
void printvect(vector<pair<int,int>> v)
{
    for(int i=0;i<v.size();i++)
    {
     cout<<v[i].first<<""<<v[i].second;
    }
    
}
int main()
{
    vector<pair<int,int>> v;
    int n;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        int x,y;
        cin>>x>>y;
        v.push_back({x,y});
    }
    printvect(v);
    return 0;
}






ðŸ˜ŽWAP array of vectors dynamically
#include<bits/stdc++.h>
using namespace std;
void printvect(vector<int> &v)
{
    for(int i=0;i<v.size();i++)
    {
     cout<<v[i]<<"";
    }
    cout<<endl;
    
}
int main()
{
    int N,i;
    cin>>N;
    vector<int> v[N];
    for(int i=0;i<N;i++)
    {
        int n;
        cin>>n;
       for(int j=0;j<n;j++)
        {
        int x;
        cin>>x;
        v[i].push_back(x);
    }
    printvect(v[i]);
    }
    return 0;
}





ðŸ˜ŽWAP vector of vector dynamically
#include<bits/stdc++.h>
using namespace std;
void printvect(vector<int> &v)
{
    for(int i=0;i<v.size();i++)
    {
        cout<<v[i]<<""<<endl;
    
    }
    
}
int main()
{
    
    vector<vector<int>> v;
    int N;
    cin>>N;
    for(int i=0;i<N;i++)
    {
        int n;
        cin>>n;
        vector<int> temp;
        for(int j=0;j<n;j++)
        {
            int x;
            cin>>x;
            temp.push_back(x);
        }
        v.push_back(temp);
    }
    for(int i=0;i<v.size();i++)
    {
        printvect(v[i]);
        cout<<endl;
    }
}
